t(() => {\n    axios\n      .get(`${api}/api/product/${id}`, {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          \"x-auth-token\": localStorage.token,\n        },\n      })\n      .then((res) => setProduct(res?.data))\n      .catch((err) => console.log(err));\n  }, [id]);\n  return (\n    <div className={classes.product}>\n      <div>\n        {!edit ? (\n          <>\n            <Button\n              color=\"primary\"\n              onClick={() => setEdit((prev) => !prev)}\n              variant=\"outlined\"\n            >\n              ویرایش محصول\n            </Button>\n            <br />\n            <br />\n            <h1>{product?.name}</h1>\n            <br />\n            <div>\n              <h3>قیمت</h3>\n              <ul>\n                <li>{product?.price}</li>\n              </ul>\n            </div>\n            <div>\n              <h3>قیمت با تخفیف</h3>\n              <ul>\n                <li>{product?.discount}</li>\n              </ul>\n            </div>\n            <div>\n              <h3>عکس های محصول</h3>\n              <ul>\n                {product?.images?.length\n                  ? product?.images?.map((el) => (\n                      <li style={{ display: \"inline-block\" }}>\n                        <img\n                          width={200}\n                          style={{ padding: 10 }}\n                          src={`${api}/uploads/admin/product/${id}/${el}`}\n                          alt={product?.name}\n                        />\n                      </li>\n                    ))\n                  : \"No Photos Added\"}\n              </ul>\n            </div>\n            <div>\n              <h3>دسته بندی</h3>\n              <ul>\n                {product?.categories?.map((el) => (\n                  <li>{el?.name}</li>\n                ))}\n              </ul>\n            </div>\n            <div>\n              <h3>تعداد موجود در انبار</h3>\n              <ul>\n                <li>{product?.stock}</li>\n              </ul>\n            </div>\n            <div>\n              <h3>سایزهای موجود</h3>\n              <ul>\n                {product?.sizes?.length\n                  ? product?.sizes?.map((el) => (\n                      <li>\n                        {el?.size} - {el?.price}\n                      </li>\n                    ))\n                  : \"No Sizes Added\"}\n              </ul>\n            </div>\n            <div>\n              <h3>تعداد دیدگاه های مشتریان برای این محصول</h3>\n              <ul>\n                <li>\n                  {product?.comments?.length\n                    ? product?.comments?.map((el) => el?.size)\n                    : \"بدون دیدگاه\"}\n                </li>\n              </ul>\n            </div>\n            <div>\n              <h3>تاریخ بارگزاری</h3>\n              <ul>{new Date(product?.createdAt).toLocaleString()}</ul>\n            </div>\n            <div>\n              <h3>تاریخ آخرین بروزرسانی</h3>\n              <ul>{new Date(product?.updatedAt).toLocaleString()}</ul>\n            </div>\n          </>\n        ) : (\n          <EditProduct product={product} setProduct={setProduct} />\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default Product;\n"]},"metadata":{},"sourceType":"module"}