rr));\n  }, []);\n\n  return (\n    <div className={classes.parent}>\n      <div className={classes.child}>\n        <div>\n          <h3>نام محصول</h3>\n        </div>\n        <div>\n          <TextField\n            variant=\"outlined\"\n            type=\"text\"\n            value={product?.name}\n            onChange={(e) => setProduct({ ...product, name: e.target.value })}\n          />\n        </div>\n      </div>\n      <div className={classes.child}>\n        <div>\n          <h3>توضیحات مختصر</h3>\n        </div>\n        <div>\n          <TextField\n            variant=\"outlined\"\n            type=\"text\"\n            value={product?.description}\n            onChange={(e) =>\n              setProduct({ ...product, description: e.target.value })\n            }\n          />\n        </div>\n      </div>\n      <div className={classes.child}>\n        <div>\n          <h3>قیمت</h3>\n        </div>\n        <div>\n          <TextField\n            variant=\"outlined\"\n            type=\"text\"\n            value={product?.price}\n            onChange={(e) =>\n              setProduct({ ...product, price: parseInt(e.target.value) })\n            }\n          />\n        </div>\n      </div>\n      <div className={classes.child}>\n        <div>\n          <h3>قیمت با تخفیف</h3>\n        </div>\n        <div>\n          <TextField\n            variant=\"outlined\"\n            type=\"text\"\n            value={product?.discount}\n            onChange={(e) =>\n              setProduct({ ...product, discount: parseInt(e.target.value) })\n            }\n          />\n        </div>\n      </div>\n      <div className={classes.child}>\n        <div>\n          <h3>افزودن عکس</h3>\n        </div>\n        <div>\n          <ul>\n            {product?.images?.length\n              ? product?.images?.map((el) => (\n                  <li style={{ display: \"inline-block\" }}>\n                    <img\n                      style={{ padding: 10 }}\n                      width={200}\n                      // src={`${api}/uploads/admin/product/${product?._id}/${el}`}\n                      alt={product?.name}\n                    />\n                  </li>\n                ))\n              : null}\n          </ul>\n        </div>\n        <div>\n          <div>\n            <input\n              type=\"file\"\n              name=\"image\"\n              id=\"image\"\n              onChange={(e) => {\n                setImageFileName(e.target.files[0]);\n                setFile(URL.createObjectURL(e.target.files[0]));\n              }}\n            />\n\n            <img src={file} />\n          </div>\n        </div>\n      </div>\n      <div className={classes.child}>\n        <div>\n          <h3>انتخاب دسته بندی</h3>\n        </div>\n        <div>\n          <ul>\n            {product?.categories?.map((el) => (\n              <li>{el?.name}</li>\n            ))}\n          </ul>\n        </div>\n        <div>\n          <div>\n            <Autocomplete\n              fullWidth\n              options={categories}\n              getOptionLabel={(option) => option.name}\n              renderInput={(params) => (\n                <TextField {...params} variant=\"outlined\" />\n              )}\n              onChange={(e, data) => {\n                setCategory(data);\n                if (sCategories.includes(data._id)) {\n                  let newC = sCategories.filter((sc) => sc !== data._id);\n                  setSCategories(newC);\n                } else {\n                  setSCategories([...sCategories, data._id]);\n                }\n              }}\n            />\n          </div>\n          <div>\n            <Button\n              color=\"primary\"\n              variant=\"outlined\"\n              onClick={() => {\n                if (category) {\n                  setProduct({\n                    ...product,\n                    categories: [...product.categories, category],\n                  });\n                } else {\n                  alert(\"دسته بندی مورد نظر را انتخاب کنید\");\n                }\n              }}\n            >\n              افزودن\n            </Button>\n          </div>\n        </div>\n      </div>\n      <div className={classes.child}>\n        <div>\n          <h3>تعداد موجودی در انبار</h3>\n        </div>\n        <div>\n          <TextField\n            variant=\"outlined\"\n            type=\"number\"\n            value={product?.stock}\n            onChange={(e) =>\n              setProduct({ ...product, stock: parseInt(e.target.value) })\n            }\n          />\n        </div>\n      </div>\n      <div className={classes.child}>\n        <div>\n          <h3>افزودن سایز</h3>\n        </div>\n        <div>\n          <ul>\n            {product?.sizes?.length\n              ? product?.sizes?.map((el) => (\n                  <li>\n                    {el?.size} - {el?.price}\n                  </li>\n                ))\n              : null}\n          </ul>\n        </div>\n        <div>\n          <div>\n            <Autocomplete\n              fullWidth\n              options={[\"بزرگ\", \"متوسط\", \"کوچک\"]}\n              getOptionLabel={(option) => option}\n              renderInput={(params) => (\n                <TextField {...params} variant=\"outlined\" />\n              )}\n              onChange={(e, data) => {\n                let d =\n                  data === \"کوچک\"\n                    ? \"small\"\n                    : data === \"متوسط\"\n                    ? \"normal\"\n                    : \"large\";\n                setSize(d);\n              }}\n            />\n          </div>\n          <div>\n            <TextField\n              variant=\"outlined\"\n              type=\"text\"\n              label=\"Price\"\n              onChange={(e) => setSizePrice(parseInt(e.target.value))}\n              value={sizePrice}\n            />\n          </div>\n          <div>\n            <Button\n              color=\"primary\"\n              variant=\"outlined\"\n              onClick={() => {\n                if (sizePrice && size) {\n                  setProduct({\n                    ...product,\n                    sizes: [...product.sizes, { size, price: sizePrice }],\n                  });\n                  setSizePrice(\"\");\n                  setSize(\"\");\n                } else {\n                  alert(\"قیمت و سایز را انتخاب کنید\");\n                }\n              }}\n            >\n              افزودن\n            </Button>\n          </div>\n        </div>\n      </div>\n      <div>\n        <Button\n          fullWidth\n          color=\"primary\"\n          variant=\"outlined\"\n          onClick={() => {\n            let data = product;\n            if (data.stock > 0) {\n              data.inStock = true;\n            } else {\n              data.inStock = true;\n            }\n\n            data.categories = sCategories;\n\n            axios\n              .post(`${api}/api/product`, data, {\n                headers: {\n                  \"Content-Type\": \"application/json\",\n                  \"x-auth-token\": localStorage.token,\n                },\n              })\n              .then((res) => {\n                if (res?.data) {\n                  var formdata = new FormData();\n                  formdata.append(\"image\", imageFileName);\n                  axios.post(\n                    `${api}/api/product/upload/${res?.data?._id}`,\n                    formdata,\n                    {\n                      headers: {\n                        \"x-auth-token\": localStorage.token,\n                      },\n                    }\n                  );\n\n                  push(\"/products\");\n                }\n              })\n              .catch((err) => console.log(err));\n          }}\n          size=\"large\"\n        >\n          افزودن محصول\n        </Button>\n      </div>\n    </div>\n  );\n};\n\nexport default AddProduct;\n"]},"