{"ast":null,"code":"import { createMuiTheme } from \"@material-ui/core/styles\";\nimport breakpoints from \"./breakpoints\";\nimport overrides from \"./overrides\";\nimport lightPalette from \"./palette\";\nimport darkPalette from \"./darkPalette\";\nimport shadows from \"./shadows\";\nimport transitions from \"./transitions\";\nimport typography from \"./typography\";\nexport var getThemeOptions = function getThemeOptions(direction, palette) {\n  var fontFamily = direction === \"rtl\" ? '\"Peyda\", \"Helvetica\", \"Arial\", sans-serif' : '\"Roboto\", \"Peyda\", \"Helvetica\", \"Arial\", sans-serif';\n  typography.fontFamily = fontFamily;\n  return {\n    direction: direction,\n    palette: palette,\n    shadows: shadows,\n    typography: typography,\n    spacing: 4,\n    transitions: transitions,\n    overrides: overrides,\n    breakpoints: breakpoints,\n    shape: {\n      borderRadius: 4\n    }\n  };\n};\nexport var rtlThemeDark = createMuiTheme(getThemeOptions(\"rtl\", darkPalette));\nexport var rtlThemeLight = createMuiTheme(getThemeOptions(\"rtl\", lightPalette));\nexport var ltrThemeDark = createMuiTheme(getThemeOptions(\"ltr\", darkPalette));\nexport var ltrThemeLight = createMuiTheme(getThemeOptions(\"ltr\", lightPalette));\nvar baseTheme = createMuiTheme(getThemeOptions(\"rtl\", lightPalette));\nexport default baseTheme;","map":{"version":3,"sources":["/home/mohammad/projects/dorreh/market multi/client/src/theme/theme.ts"],"names":["createMuiTheme","breakpoints","overrides","lightPalette","darkPalette","shadows","transitions","typography","getThemeOptions","direction","palette","fontFamily","spacing","shape","borderRadius","rtlThemeDark","rtlThemeLight","ltrThemeDark","ltrThemeLight","baseTheme"],"mappings":"AAAA,SACEA,cADF,QAIO,0BAJP;AAKA,OAAOC,WAAP,MAAwB,eAAxB;AACA,OAAOC,SAAP,MAAsB,aAAtB;AACA,OAAOC,YAAP,MAAyB,WAAzB;AACA,OAAOC,WAAP,MAAwB,eAAxB;AACA,OAAOC,OAAP,MAAoB,WAApB;AACA,OAAOC,WAAP,MAAwB,eAAxB;AACA,OAAOC,UAAP,MAAuB,cAAvB;AAEA,OAAO,IAAMC,eAAe,GAAG,SAA